{
  "name": "werift",
  "version": "0.0.31",
  "description": "WebRTC Implementation for TypeScript (Node.js)",
  "keywords": [
    "WebRTC",
    "node.js"
  ],
  "homepage": "https://github.com/shinyoshiaki/rainy-webrtc",
  "repository": {
    "type": "git",
    "url": "https://github.com/shinyoshiaki/rainy-webrtc.git"
  },
  "license": "MIT",
  "author": {
    "name": "shinyoshiaki"
  },
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "answer": "ts-node --files examples/datachannel/answer.ts",
    "build": "rm -rf lib && tsc",
    "demo": "ts-node --files examples/mediachannel/simulcast/select.ts",
    "depcheck": "depcheck",
    "deploy": "./build.sh && yarn publish",
    "doc": "rm -rf doc && typedoc --mode file --out doc ./src/**/*.ts",
    "lint": "eslint src/**/*.ts --fix",
    "format": "yarn unused && yarn lint && prettier --write src/**/*.ts",
    "media": "ts-node --files examples/mediachannel/sendrecv/offer.ts",
    "offer": "ts-node --files examples/datachannel/offer.ts",
    "test": "npm run type && jest --runInBand --forceExit",
    "type": "tsc --noEmit --project ./tsconfig.json",
    "unused": "organize-imports-cli src/**/*.ts"
  },
  "dependencies": {
    "@fidm/x509": "^1.2.1",
    "aes-js": "^3.1.2",
    "big-integer": "^1.6.48",
    "binary-data": "^0.6.0",
    "buffer-crc32": "^0.2.13",
    "debug": "^4.3.1",
    "elliptic": "^6.5.3",
    "int64-buffer": "^1.0.0",
    "ip": "^1.1.5",
    "jspack": "^0.0.4",
    "lodash": "^4.17.20",
    "p-cancelable": "^2.0.0",
    "rx.mini": "^1.0.0",
    "turbo-crc32": "^1.0.1",
    "tweetnacl": "^1.0.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@discordjs/opus": "^0.3.3",
    "@types/aes-js": "^3.1.1",
    "@types/big-integer": "^0.0.31",
    "@types/buffer-crc32": "^0.2.0",
    "@types/debug": "^4.1.5",
    "@types/elliptic": "^6.4.12",
    "@types/express": "^4.17.9",
    "@types/ip": "^1.1.0",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.14",
    "@types/socket.io-client": "^1.4.34",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "python-shell": "^2.0.3",
    "socket.io-client": "^3.0.4",
    "speaker": "^0.5.2",
    "typedoc": "^0.19.2",
    "ws": "^7.4.1"
  },
  "engines": {
    "node": ">=14"
  },
  "readme": "README.md"
}
