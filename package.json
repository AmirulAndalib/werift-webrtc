  "dependencies": {
    "@fidm/x509": "^1.2.1",
    "binary-data": "^0.6.0",
    "buffer-crc32": "^0.2.13",
    "date-fns": "^2.13.0",
    "dgram": "^1.0.1",
    "elliptic": "^6.5.2",
    "icet": "^0.0.3",
    "int64-buffer": "^0.99.1007",
    "ip": "^1.1.5",
    "jspack": "^0.0.4",
    "lodash": "^4.17.15",
    "node-forge": "^0.9.1",
    "p-cancelable": "^2.0.0",
    "rxjs": "^6.5.5",
    "turbo-crc32": "^1.0.1"
  },
  "devDependencies": {
    "@types/buffer-crc32": "^0.2.0",
    "@types/elliptic": "^6.4.12",
    "@types/ip": "^1.1.0",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.150",
    "@types/node": "^14.0.1",
    "@types/node-forge": "^0.9.3",
    "@types/utf8": "^2.1.6",
    "@types/ws": "^7.2.4",
    "jest": "^26.0.0",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.1",
    "tslib": "^1.12.0",
    "typescript": "^3.9.2",
    "ws": "^7.3.0"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/*.test.ts"
    ]
  }
}
